<!DOCTYPE html>
<html lang="cs">
	<head>
		 <meta charset="utf-8">
		 <title>{$title}</title>
		 {wpm_script $basePath, '/dist/css/tisk_css.css'}
	</head>
	<body class="{$format} tisk">
	{foreach $pages as $c}
		{$c->getNode()->startTag()|noescape}
