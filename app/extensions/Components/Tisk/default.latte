	{/foreach}
	<div id="buttons">
		{foreach $buttons as $b}
		{$b}
		{/foreach}
		<a href="javascript:print();" class="btn btn-primary">Tisk</a>
		<a n:if="$back" href="{$back}" class="btn btn-primary">ZpÄ›t</a>
	</div>
	</body>
</html>